      "tidalcycles:reboot"
    ]
  },
  "dependencies": {
    "atom-space-pen-views": "^2.0.3",
    "osc": "^2.4.0"
  },
  "repository": "https://github.com/tidalcycles/atom-tidalcycles",
  "license": "GPL-3.0",
  "engines": {
    "atom": ">=0.174.0 <2.0.0"
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "autocompleteProvider"
      }
    }
  }
}
